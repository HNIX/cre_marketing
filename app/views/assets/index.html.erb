<h1>Listing assets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Highlights</th>
      <th>Size</th>
      <th>Size type</th>
      <th>Price</th>
      <th>Price psf</th>
      <th>Dnd price</th>
      <th>Cap rate</th>
      <th>Occupancy</th>
      <th>Year built</th>
      <th>Offer date</th>
      <th>Title</th>
      <th>Visibility</th>
      <th>Ended</th>
      <th>Address</th>
      <th>Address 2</th>
      <th>State</th>
      <th>City</th>
      <th>Zip</th>
      <th>Website</th>
      <th>Country</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @assets.each do |asset| %>
      <tr>
        <td><%= asset.name %></td>
        <td><%= asset.description %></td>
        <td><%= asset.highlights %></td>
        <td><%= asset.size %></td>
        <td><%= asset.size_type %></td>
        <td><%= asset.price %></td>
        <td><%= asset.price_psf %></td>
        <td><%= asset.dnd_price %></td>
        <td><%= asset.cap_rate %></td>
        <td><%= asset.occupancy %></td>
        <td><%= asset.year_built %></td>
        <td><%= asset.offer_date %></td>
        <td><%= asset.title %></td>
        <td><%= asset.visibility %></td>
        <td><%= asset.ended %></td>
        <td><%= asset.address %></td>
        <td><%= asset.address_2 %></td>
        <td><%= asset.state %></td>
        <td><%= asset.city %></td>
        <td><%= asset.zip %></td>
        <td><%= asset.website %></td>
        <td><%= asset.country %></td>
        <td><%= asset.user %></td>
        <td><%= link_to 'Show', asset %></td>
        <td><%= link_to 'Edit', edit_asset_path(asset) %></td>
        <td><%= link_to 'Destroy', asset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Asset', new_asset_path %>

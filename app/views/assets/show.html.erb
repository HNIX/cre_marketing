<%- model_class = Asset -%>
<div class="page-header">
  <h1><%=t '.title', :default => Asset %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:name) %></dt>
      <dd><pre class="prettyprint"><%= @asset.name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:description) %></dt>
      <dd><pre class="prettyprint"><%= @asset.description %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:highlights) %></dt>
      <dd><pre class="prettyprint"><%= @asset.highlights %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:size) %></dt>
      <dd><pre class="prettyprint"><%= @asset.size %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:size_type) %></dt>
      <dd><pre class="prettyprint"><%= @asset.size_type %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:price) %></dt>
      <dd><pre class="prettyprint"><%= @asset.price %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:price_psf) %></dt>
      <dd><pre class="prettyprint"><%= @asset.price_psf %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:dnd_price) %></dt>
      <dd><pre class="prettyprint"><%= @asset.dnd_price %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:cap_rate) %></dt>
      <dd><pre class="prettyprint"><%= @asset.cap_rate %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:occupancy) %></dt>
      <dd><pre class="prettyprint"><%= @asset.occupancy %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:year_built) %></dt>
      <dd><pre class="prettyprint"><%= @asset.year_built %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:offer_date) %></dt>
      <dd><pre class="prettyprint"><%= @asset.offer_date %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:title) %></dt>
      <dd><pre class="prettyprint"><%= @asset.title %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:visibility) %></dt>
      <dd><pre class="prettyprint"><%= @asset.visibility %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:ended) %></dt>
      <dd><pre class="prettyprint"><%= @asset.ended %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:address) %></dt>
      <dd><pre class="prettyprint"><%= @asset.address %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:address_2) %></dt>
      <dd><pre class="prettyprint"><%= @asset.address_2 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:state) %></dt>
      <dd><pre class="prettyprint"><%= @asset.state %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:city) %></dt>
      <dd><pre class="prettyprint"><%= @asset.city %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zip) %></dt>
      <dd><pre class="prettyprint"><%= @asset.zip %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:website) %></dt>
      <dd><pre class="prettyprint"><%= @asset.website %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:country) %></dt>
      <dd><pre class="prettyprint"><%= @asset.country %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              assets_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_asset_path(@asset), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              asset_path(@asset),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

